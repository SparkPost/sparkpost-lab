{
  "name": "sparkpost-lab",
  "version": "0.1.0",
  "engines": {
    "node": "6.9.2"
  },
  "private": true,
  "devDependencies": {
    "concurrently": "^3.1.0",
    "nodemon": "^1.11.0",
    "react-scripts": "0.8.5"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "fuse-email": "^0.0.1-alpha.2",
    "knex": "^0.12.6",
    "pg": "^6.1.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0"
  },
  "scripts": {
    "database:latest": "./bin/database.sh && ./node_modules/.bin/knex migrate:latest",
    "database:rollback": "./node_modules/.bin/knex migrate:rollback",
    "dev": "concurrently --kill-others \"npm run server\" \"npm start\"",
    "server": "nodemon --watch server server",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "heroku-postbuild": "npm run build"
  }
}
